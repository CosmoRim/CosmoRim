<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<ThingDef ParentName="BuildingBase">
		<defName>CosRim_FriendlyOrderCrate</defName>
		<label>Friendly Order Crate</label>
		<description>Sturdy packaging to keep your delivery safe! Requires manual opening when you are ready. Due to strict eco preservation laws in the sector, the packaging crate is prepared to completely disintigrate on opening so there's no mess left over!</description>
		<!-- <thingClass>CosmoCore.Building_OrderKiosk</thingClass> -->
		<graphicData>
			<texPath>CosmoRim/Buildings/PackagingCrate</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(3,3)</drawSize>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<pathCost>50</pathCost>
		<blockWind>true</blockWind>
		<castEdgeShadows>false</castEdgeShadows>
		<fillPercent>0.5</fillPercent>
		<canOverlapZones>false</canOverlapZones>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>BuildingsMisc</li>
		</thingCategories>
		<statBases>
			<MaxHitPoints>200</MaxHitPoints>
			<WorkToBuild>2200</WorkToBuild>
			<Flammability>0.3</Flammability>
			<Mass>20</Mass>
		</statBases>
		<size>(1,1)</size>
		<building>
			<destroySound>BuildingDestroyed_Metal_Big</destroySound>
		</building>
		<costList>
			<Steel>1</Steel>
		</costList>
		<rotatable>false</rotatable>
		<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
		<designationCategory>CosRim_CosmoRim</designationCategory>
		<modExtensions>
			<li Class="TabulaRasa.DefModExt_SubCategoryBuilding">
				<subCategory>CosRim_SubCatMisc</subCategory>
				<showOnlyInCategory>true</showOnlyInCategory>
			</li>
		</modExtensions>
	</ThingDef>

</Defs>