<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<ThingDef ParentName="UnfinishedBase">
		<defName>CosRim_UnfinishedVRChip</defName>
		<label>unfinished vr chip</label>
		<description>An unfinished VR chip.</description>
		<statBases>
			<Flammability>0.3</Flammability>
		</statBases>
		<graphicData>
			<texPath>CosmoRim/Items/UnfinishedVRChip</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<stuffCategories Inherit="false" />
	</ThingDef>

	<ThingDef ParentName="ResourceBase">
		<defName>CosRim_VRChip_Blank</defName>
		<label>blank vr chip</label>
		<description>A hi-tech VR chip capable of recording various VR training skill guides by a planetary class masters of the craft.</description>
		<graphicData>
			<texPath>CosmoRim/Items/VRChip</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<soundInteract>Standard_Drop</soundInteract>
		<soundDrop>Standard_Drop</soundDrop>
		<stackLimit>25</stackLimit>
		<healthAffectsPrice>false</healthAffectsPrice>
		<statBases>
			<MaxHitPoints>20</MaxHitPoints>
			<MarketValue>350</MarketValue>
			<Mass>0.5</Mass>
			<Flammability>0.8</Flammability>
			<DeteriorationRate>2.0</DeteriorationRate>
		</statBases>
		<intricate>true</intricate>
		<thingCategories>
			<li>CosRim_VRChips</li>
		</thingCategories>
	</ThingDef>

</Defs>